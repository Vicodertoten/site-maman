{
  "name": "gastronomade-site",
  "type": "module",
  "version": "1.0.0",
  "description": "Site web Gastronomade - Manger Vrai avec Muriel Cruysmans",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "seo:qa:sitemap": "node scripts/qa-sitemap.mjs --strict --report ./.reports/seo-sitemap-qa.json",
    "seo:qa:sitemap:local": "node scripts/qa-sitemap.mjs --skip-http --sitemap-file ./dist/sitemap.xml --report ./.reports/seo-sitemap-qa.local.json",
    "studio": "cd studio && npm run dev",
    "studio:build": "cd studio && npm run build",
    "lint": "eslint . --ext .js,.ts,.astro",
    "lint:fix": "eslint . --ext .js,.ts,.astro --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/netlify": "^6.6.4",
    "@astrojs/sitemap": "^3.7.0",
    "@sanity/client": "^6.29.1",
    "@supabase/supabase-js": "^2.49.0",
    "astro": "^5.16.15",
    "resend": "^6.0.2",
    "sharp": "^0.34.5",
    "stripe": "^14.25.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "gastronomie",
    "cuisine",
    "sant\u00e9",
    "recettes",
    "coaching"
  ],
  "author": "Muriel Cruysmans",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.19"
  }
}
